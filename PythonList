data = [1,1,'A','B','C']
for x in data:
    print (x)


##Appending data to list
data.append(34)
print ("After Appending the data")
for x in data:
    print (x)

#Inserting data to  list
data.insert(0,"Anand")
print ("After inserting element")
for x in data:
    print (x)

#Deleting the element from list
print ("After removing  element")
del data[0]
for x  in data:
    print (x)
